{
  "name": "organizer-service",
  "main": "src/index.ts",
  "compatibility_date": "2024-01-01",

  // Environment variables
  "vars": {
    "DEEPINFRA_BASE_URL": "https://api.deepinfra.com/v1/openai",
    "MODEL_NAME": "deepseek-ai/DeepSeek-V3",
    "MAX_TOKENS": 8192
  },

  // Note: Set DEEPINFRA_API_KEY as a secret using:
  // wrangler secret put DEEPINFRA_API_KEY

  // Build configuration
  "build": {
    "command": "npm run build"
  },

  // Domain configuration
  "routes": [
    {
      "pattern": "organizer.arke.institute",
      "custom_domain": true
    }
  ],

  // Observability
  "observability": {
    "enabled": true
  }
}
